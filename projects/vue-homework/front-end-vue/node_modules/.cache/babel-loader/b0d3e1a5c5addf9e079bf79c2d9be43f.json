{"ast":null,"code":"import { reactive } from \"vue\"; //컴포넌트 반응형 속성 선언\n\nexport default {\n  setup(__props, {\n    expose\n  }) {\n    expose();\n    const product = reactive({\n      no: 1,\n      name: \"미니백\",\n      company: \"클레인\",\n      price: 300000,\n      image: \"minibag-black.png\",\n      detail: {\n        info: `\n    <div>\n      시그너츠 Cecylde Lock 마그네틱 클로저가 특징입니다.\n    </div>  \n  `,\n        sale: false\n      }\n    }); //메소드 선언\n\n    function getPrice() {\n      return product.price;\n    }\n\n    function changeDate() {\n      //반응형 객체의 내부 속성값 변경\n      product.no++;\n      product.name = \"빨간 미니백\";\n      product.price += 1000;\n      product.image = \"minibag-red.png\";\n      product.detail.info = \"<div>시그너츠 Cecylde Lock 마그네틱 클로저와 빨간 색의 조화가 좋습니다.</div> \";\n      product.detail.sale = !product.detail.sale; //반응형 객체 자체를 바꿀 수는 없음\n    }\n\n    const __returned__ = {\n      product,\n      getPrice,\n      changeDate,\n      reactive\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n\n};","map":{"version":3,"mappings":"AAyBA,SAASA,QAAT,QAAyB,KAAzB,C,CACA;;;;;;;IACA,MAAMC,OAAO,GAAGD,QAAQ,CAAC;MACrBE,EAAE,EAAE,CADiB;MAErBC,IAAI,EAAE,KAFe;MAGrBC,OAAO,EAAE,KAHY;MAIrBC,KAAK,EAAE,MAJc;MAKrBC,KAAK,EAAE,mBALc;MAMrBC,MAAM,EAAE;QACJC,IAAI,EAAG;AACf;AACA;AACA;AACA,GALY;QAMJC,IAAI,EAAE;MANF;IANa,CAAD,CAAxB,EAgBA;;IACA,SAASC,QAAT,GAAoB;MAChB,OAAOT,OAAO,CAACI,KAAf;IACH;;IAED,SAASM,UAAT,GAAsB;MAClB;MACAV,OAAO,CAACC,EAAR;MACAD,OAAO,CAACE,IAAR,GAAe,QAAf;MACAF,OAAO,CAACI,KAAR,IAAiB,IAAjB;MACAJ,OAAO,CAACK,KAAR,GAAgB,iBAAhB;MACAL,OAAO,CAACM,MAAR,CAAeC,IAAf,GAAsB,yDAAtB;MACAP,OAAO,CAACM,MAAR,CAAeE,IAAf,GAAsB,CAACR,OAAO,CAACM,MAAR,CAAeE,IAAtC,CAPkB,CASlB;IACH","names":["reactive","product","no","name","company","price","image","detail","info","sale","getPrice","changeDate"],"sources":["/Users/jbc/Desktop/osstem/projects/node-workspace/front-end-vue/front-end-vue/src/views/menu02/Exam03ReactiveData.vue"],"sourcesContent":["<template>\n    <div class=\"card\">\n        <div class=\"card-header\">exam08Binding</div>\n        <div class=\"card-body\">\n            <p>번호: {{ product.no }}</p>\n            <p>이름: {{ product.name }}</p>\n            <p>회사: {{ product.company }}</p>\n            <p>가격: {{ getPrice() }}</p>\n            <p>판매: {{ product.detail.sale ? \"판매\" : \"품절\" }}</p>\n            <hr />\n            <p v-html=\"product.detail.info\"></p>\n            <hr />\n            <p>\n                <img src=\"@/assets/products/minibag-black.png\" class=\"mr-2\" height=\"150\" />\n                <img v-bind:src=\"require(`@/assets/products/${product.image}`)\" height=\"150\" />\n                <span v-if=\"!product.detail.sale\"><img src=\"@/assets/products/soldout.jpg\" class=\"mr-2\" width=\"100\" /></span>\n                <span v-show=\"!product.detail.sale\"><img src=\"@/assets/products/soldout.jpg\" class=\"mr-2\" width=\"100\" /></span>\n            </p>\n            <hr />\n            <button @click=\"changeDate\" class=\"btn btn-info btn-sm\">데이터 변경</button>\n        </div>\n    </div>\n</template>\n\n<script setup>\nimport { reactive } from \"vue\";\n//컴포넌트 반응형 속성 선언\nconst product = reactive({\n    no: 1,\n    name: \"미니백\",\n    company: \"클레인\",\n    price: 300000,\n    image: \"minibag-black.png\",\n    detail: {\n        info: `\n    <div>\n      시그너츠 Cecylde Lock 마그네틱 클로저가 특징입니다.\n    </div>  \n  `,\n        sale: false,\n    },\n});\n\n//메소드 선언\nfunction getPrice() {\n    return product.price;\n}\n\nfunction changeDate() {\n    //반응형 객체의 내부 속성값 변경\n    product.no++;\n    product.name = \"빨간 미니백\";\n    product.price += 1000;\n    product.image = \"minibag-red.png\";\n    product.detail.info = \"<div>시그너츠 Cecylde Lock 마그네틱 클로저와 빨간 색의 조화가 좋습니다.</div> \";\n    product.detail.sale = !product.detail.sale;\n\n    //반응형 객체 자체를 바꿀 수는 없음\n}\n</script>\n\n<style scoped></style>\n"]},"metadata":{},"sourceType":"module"}