{"ast":null,"code":"import { ref } from \"vue\"; //반응형 속성선언\n\nexport default {\n  setup(__props, {\n    expose\n  }) {\n    expose();\n    const userId = ref(\"홍킬동\");\n\n    function handleInput(event) {\n      console.log(\"handleInput(evet) 실행\");\n      console.log(event.target);\n      console.log(event.target.value);\n      console.log(event.target.name);\n    } //메소드 선언\n\n\n    function handleBtn1() {\n      console.log(\"handleBtn1() 실행\");\n    }\n\n    function handleBtn2(message, event) {\n      console.log(\"handleBtn2() 실행\");\n      console.log(\"message: \" + message);\n      console.log(event);\n      console.log(event.target);\n      console.log(event.type);\n    }\n\n    const __returned__ = {\n      userId,\n      handleInput,\n      handleBtn1,\n      handleBtn2,\n      ref\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n\n};","map":{"version":3,"mappings":"AA6BA,SAASA,GAAT,QAAoB,KAApB,C,CAEA;;;;;;;IACA,MAAMC,MAAM,GAAGD,GAAG,CAAC,KAAD,CAAlB;;IAEA,SAASE,WAAT,CAAqBC,KAArB,EAA4B;MACxBC,OAAO,CAACC,GAAR,CAAY,sBAAZ;MACAD,OAAO,CAACC,GAAR,CAAYF,KAAK,CAACG,MAAlB;MACAF,OAAO,CAACC,GAAR,CAAYF,KAAK,CAACG,MAAN,CAAaC,KAAzB;MACAH,OAAO,CAACC,GAAR,CAAYF,KAAK,CAACG,MAAN,CAAaE,IAAzB;IACH,EAED;;;IACA,SAASC,UAAT,GAAsB;MAClBL,OAAO,CAACC,GAAR,CAAY,iBAAZ;IACH;;IAED,SAASK,UAAT,CAAoBC,OAApB,EAA6BR,KAA7B,EAAoC;MAChCC,OAAO,CAACC,GAAR,CAAY,iBAAZ;MACAD,OAAO,CAACC,GAAR,CAAY,cAAcM,OAA1B;MACAP,OAAO,CAACC,GAAR,CAAYF,KAAZ;MACAC,OAAO,CAACC,GAAR,CAAYF,KAAK,CAACG,MAAlB;MACAF,OAAO,CAACC,GAAR,CAAYF,KAAK,CAACS,IAAlB;IAEH","names":["ref","userId","handleInput","event","console","log","target","value","name","handleBtn1","handleBtn2","message","type"],"sources":["/Users/jbc/Desktop/osstem/projects/node-workspace/front-end-vue/front-end-vue/src/views/menu03/Exam01EventHandling.vue"],"sourcesContent":["<template>\n    <div class=\"card\">\n        <div class=\"card-header\">Exam01EventHandling</div>\n        <div class=\"card-body\">\n            <div>\n                <h6>[Event Handling]</h6>\n                <button @click=\"handleBtn1\" class=\"btn btn-info btn-sm mr-2\">버튼1</button>\n                <button @click=\"handleBtn2('vue is good', $event)\" class=\"btn btn-info btn-sm\">버튼2</button>\n            </div>\n            <div>\n                <h6>[Dom event객체 참조]</h6>\n                <input type=\"text\" name=\"userId\" v-model=\"userId\" @keyup=\"handleInput($event)\" />\n                <br />\n                입력 내용: {{ userId }}\n            </div>\n\n            <div>\n                <h6>[이벤트 수식어 사용]</h6>\n                <a href=\"https://vuejs.org/guide/introduction.html\" @click.prevent=\"handleLink\">링크</a>\n                <form action=\"https://vuejs.org/guide/introduction.html\" @submit.prevent=\"handleForm\">\n                    <input type=\"text\" v-model=\"userId\" /> <br />\n                    <input type=\"submit\" value=\"제출\" class=\"btn btn-success btn-sm mt-2\" />\n                </form>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script setup>\nimport { ref } from \"vue\";\n\n//반응형 속성선언\nconst userId = ref(\"홍킬동\");\n\nfunction handleInput(event) {\n    console.log(\"handleInput(evet) 실행\");\n    console.log(event.target);\n    console.log(event.target.value);\n    console.log(event.target.name);\n}\n\n//메소드 선언\nfunction handleBtn1() {\n    console.log(\"handleBtn1() 실행\");\n}\n\nfunction handleBtn2(message, event) {\n    console.log(\"handleBtn2() 실행\");\n    console.log(\"message: \" + message);\n    console.log(event);\n    console.log(event.target);\n    console.log(event.type);\n    \n}\n</script>\n\n<style scoped></style>\n"]},"metadata":{},"sourceType":"module"}