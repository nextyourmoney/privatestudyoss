{"ast":null,"code":"import { onUpdated, onBeforeUpdate, ref } from \"vue\"; //반응형 속성 선언\n\nexport default {\n  setup(__props, {\n    expose\n  }) {\n    expose();\n    const count = ref(0); //LifeCycle Hook 호출\n\n    onBeforeUpdate(() => {\n      console.log(\"onBeforeUpdate 실행\");\n    });\n    onUpdated(() => {\n      console.log(\"onUpdated 실행\");\n    });\n    const __returned__ = {\n      count,\n      onUpdated,\n      onBeforeUpdate,\n      ref\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n\n};","map":{"version":3,"mappings":"AAaI,SAASA,SAAT,EAAoBC,cAApB,EAAoCC,GAApC,QAA+C,KAA/C,C,CAEA;;;;;;;IACA,MAAMC,KAAK,GAAGD,GAAG,CAAC,CAAD,CAAjB,EAEA;;IACAD,cAAc,CAAC,MAAI;MACfG,OAAO,CAACC,GAAR,CAAY,mBAAZ;IACH,CAFa,CAAd;IAIAL,SAAS,CAAC,MAAK;MACXI,OAAO,CAACC,GAAR,CAAY,cAAZ;IACH,CAFQ,CAAT","names":["onUpdated","onBeforeUpdate","ref","count","console","log"],"sources":["/Users/jbc/Desktop/osstem/projects/node-workspace/front-end-vue/front-end-vue/src/views/menu05/Exam01LifeCycleHooks/UpdateHook.vue"],"sourcesContent":["<template>\n    <div class=\"card\">\n        <div class = \"card-header\">\n            UpdateHook\n        </div>\n        <div class=\"card-body\">\n            <button @click = \"count++\" class = \"btn btn-sm btn-info mr-2\"> 증가 </button>\n            <button @click = \"count++\" class = \"btn btn-sm btn-info mr-2\"> 증가 </button>\n        </div>\n    </div>\n</template>\n\n<script setup>\n    import { onUpdated, onBeforeUpdate, ref } from \"vue\";\n\n    //반응형 속성 선언\n    const count = ref(0);\n\n    //LifeCycle Hook 호출\n    onBeforeUpdate(()=>{\n        console.log(\"onBeforeUpdate 실행\");\n    });\n\n    onUpdated(() =>{\n        console.log(\"onUpdated 실행\");\n    });\n</script>\n\n<style scoped>\n\n</style>"]},"metadata":{},"sourceType":"module"}